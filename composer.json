{
    "name": "vantezzen/journal",
    "description": "Journal is a simple CMS for static blogs. It aims to simplify the creation and maintenance of blogs, letting the author focus on writing posts instead of wasting time on the CMS.",
    "version": "1.2.0",
    "type": "project",
    "require": {
        "vowserdb/vowserdb": "^4.0",
        "slim/slim": "^3.0",
        "suin/php-rss-writer": "^1.6",
        "cocur/slugify": "^3.1",
        "league/flysystem": "^1.0",
        "league/flysystem-sftp": "^1.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "misd/linkify": "^1.1",
        "erusev/parsedown": "^1.7",
        "jenssegers/blade": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7"
    },
    "autoload": {
        "psr-4": {
            "core\\": "core"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "vantezzen",
            "email": "vantezzen@gmail.com"
        }
    ],
    "scripts": {
        "test": [
            "./vendor/bin/phpunit --bootstrap vendor/autoload.php tests/"
        ]
    }
}
